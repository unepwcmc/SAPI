<strong class="title more">CMS Agreements and MoUs</strong>
{{#unless cmsInstruments}}
  <p class="empty">This species is not currently listed in any CMS Agreement or MoU.</p>
{{else}}
  <table summary="data table">
    <thead>
      <tr>
        <th class="col1">&nbsp;</th>
        <th class="col2">NAME</th>
        <th class="col3">DATE</th>
        <th class="col1">&nbsp;</th>
      </tr>
    </thead>
    <tbody>
      {{#each agreement in cmsInstruments}}
          <tr class="current">
            <td>&nbsp;</td>
            <td>{{agreement.name}}</td>
            <td>{{agreement.effective_from_formatted}}</td>
            <td class="last">&nbsp;</td>
          </tr>
      {{/each}}
    </tbody>
  </table>
{{/unless}}