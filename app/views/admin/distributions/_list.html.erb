<% @distributions.each do |d| %>
  <% if d.references.any? %>
    <%= link_to d.geo_entity.name,
          "#",
          :data => {
            :toggle => "popover",
            :"original-title" => "References",
            :html => true,
            :content => render("admin/distributions/references", :references => d.references).html_safe
          } %>
  <% else %>
    <%= d.geo_entity.name %>
  <% end %>
  <%= d.tag_list %>
  <%= link_to edit_icon, edit_admin_taxon_concept_distribution_url(@taxon_concept, d), :remote => true %>
  <%= link_to delete_icon, admin_taxon_concept_distribution_url(@taxon_concept, d), :confirm => "Are you sure?", :method => :delete %>
  <br>
<% end %>
